{
  "name": "@0xobelisk/aptos-cli",
  "version": "0.0.1",
  "description": "Tookit for interacting with move eps framework",
  "keywords": [
    "aptos",
    "obelisk labs",
    "move",
    "blockchain"
  ],
  "type": "module",
  "author": "team@obelisk.build",
  "homepage": "https://github.com/0xobelisk/obelisk-engine/tree/main/package/@0xobelisk/client#readme",
  "bugs": "https://github.com/0xobelisk/obelisk-engine/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/0xobelisk/obelisk-engine.git"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "types": "src/index.ts",
  "bin": {
    "obelisk": "./dist/obelisk.js"
  },
  "scripts": {
    "build": "pnpm run build:js",
    "build:js": "tsup && chmod +x ./dist/obelisk.js",
