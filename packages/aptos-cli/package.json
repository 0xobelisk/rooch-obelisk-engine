    "@types/openurl": "^1.0.0",
    "@types/throttle-debounce": "^5.0.0",
    "@types/yargs": "^17.0.10",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "vitest": "0.31.4"
