    "child_process": "^1.0.2",
    "chokidar": "^3.5.3",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "esbuild": "^0.17.15",
    "execa": "^7.0.0",
    "find-up": "^6.3.0",
    "glob": "^8.0.3",
    "nice-grpc-web": "^2.0.1",
    "openurl": "^1.1.1",
    "path": "^0.12.7",
    "prettier": "^2.8.4",
    "prettier-plugin-rust": "^0.1.9",
    "throttle-debounce": "^5.0.0",
    "typechain": "^8.1.1",
    "typescript": "5.1.6",
    "yargs": "^17.7.1",
    "zod": "^3.21.4",
    "zod-validation-error": "^1.3.0"
  },
  "devDependencies": {
